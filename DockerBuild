pipeline {
    agent any
       
    stages {
        stage('DockerBuild') 
        {
            steps 
            {
                sh 'ls -l'                
                sh 'echo "Shanu@2016" | sudo -S sudo docker build -t webappjenkins .'
                
            }
        }
        stage('Docker Run') 
        {
            steps 
            {
                sh 'docker run -p 80:80 webappjenkins'
            }
        }
    }
}
