pipeline {
    agent any
       
    stages {
        stage('DockerBuild') 
        {
            steps 
            {
                sh 'sudo docker build -t webappjenkins .'
                
            }
        }
        stage('Docker Run') 
        {
            steps 
            {
                sh 'sudo docker run -p 80:80 webappjenkins'
            }
        }
    }
}
